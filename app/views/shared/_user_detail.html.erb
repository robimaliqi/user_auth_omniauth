 <% if user_signed_in? %>
<%= button_to(
        "Log Out",
        destroy_user_session_path,
        method: :delete
      ) %>
 <% else %>
  <%= link_to 'Log in', new_user_session_path %>
  <%= button_to  'Login with Google', user_google_oauth2_omniauth_authorize_path, method: :post, :data => {turbo: "false"} %>
 <% end %>
